  FormBox[
   RowBox[{"t", "=", 
    RowBox[{
     SqrtBox[
      FractionBox[
       RowBox[{"2", "D"}], "a"]], "=", 
     SqrtBox[
      FractionBox[
       RowBox[{"2", "D"}], 
       RowBox[{"g", " ", 
        RowBox[{"sin", "(", "\[Theta]", ")"}]}]]]}]}], TraditionalForm]], 
  "InlineMath"],
 Cell[BoxData[
  FormBox[".", TraditionalForm]]]
}], "DetailNotes",
 CellChangeTimes->{
  3.35696210375764*^9, 3.4247129066335254`*^9, 3.4247168832005577`*^9, {
   3.4247185750012074`*^9, 3.424718579907426*^9}, {3.4247792631892405`*^9, 
   3.424779276962618*^9}, {3.4247965869539137`*^9, 3.424796599031498*^9}},
 CellID->185150589],

Cell[TextData[{
 "This is the final speed ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"(", 
     SubscriptBox["v", "f"], ")"}], " "}], TraditionalForm]], "InlineMath"],
 "at the bottom (or at any time ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]], "InlineMath"],
 "), assuming that the block starts from rest ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["v", "o"], "=", "0"}], ")"}], TraditionalForm]], 
  "InlineMath"],
 ", from the definition of acceleration: ",
 Cell[BoxData[
  FormBox[
   RowBox[{"a", "=", 
    RowBox[{
     FractionBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["v", "f"], " ", "-", " ", 
        SubscriptBox["v", "o"]}], ")"}], "t"], "=", 
     FractionBox[
      SubscriptBox["v", "f"], "t"]}]}], TraditionalForm]], "InlineMath"],
 ", or ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["v", "f"], "=", 
    RowBox[{"a", " ", "t"}]}], TraditionalForm]], "InlineMath"],
 ", and, substituting for ",
 Cell[BoxData[
  FormBox["D", TraditionalForm]], "InlineMath"],
 " and ",
 Cell[BoxData[
  FormBox["t", TraditionalForm]], "InlineMath"],
 " from above, "
}], "DetailNotes",
 CellChangeTimes->{
  3.35696210375764*^9, {3.42471679121736*^9, 3.424716850185355*^9}, {
   3.4247168880598707`*^9, 3.4247168959972687`*^9}, {3.4247187590625296`*^9, 
   3.4247187668437295`*^9}, {3.4247792904209633`*^9, 3.424779321250046*^9}, {
