  3.35696210375764*^9, {3.4247129066335254`*^9, 3.4247129066335254`*^9}, 
   3.424716695827956*^9, 3.4247794141953397`*^9, {3.4247963489333277`*^9, 
   3.4247963754477654`*^9}, {3.4250380880985785`*^9, 3.4250380884816165`*^9}},
 
 CellID->751220510],

Cell[TextData[{
 Cell[BoxData[
  FormBox[
   RowBox[{"D", "=", 
    RowBox[{
     FractionBox["1", "2"], "a", " ", 
     SuperscriptBox["t", "2"]}]}], TraditionalForm]], "InlineMath"],
 ", or "
}], "DetailNotes",
 CellChangeTimes->{
  3.35696210375764*^9, 3.4247129066335254`*^9, {3.4247168832005577`*^9, 
   3.4247168832005577`*^9}, {3.424718529189001*^9, 3.42471853636083*^9}, {
   3.4247792555884805`*^9, 3.424779258690791*^9}},
 CellID->35783750],

