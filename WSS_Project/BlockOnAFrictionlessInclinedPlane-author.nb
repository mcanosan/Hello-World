  FormBox[
   RowBox[{
    SubscriptBox["v", "f"], "=", 
    SqrtBox[
     RowBox[{"2", "g", " ", "D", " ", 
      RowBox[{"sin", "(", "\[Theta]", ")"}]}]]}], TraditionalForm]], 
  "InlineMath"],
 ". "
}], "DetailNotes",
 CellChangeTimes->{
  3.35696210375764*^9, {3.42471679121736*^9, 3.424716850185355*^9}, {
